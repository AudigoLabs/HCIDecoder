{
  "name": "UART HCI Decoder",
  "apiVersion": "1.0.0",
  "author": "Brian Gomberg",
  "version": "0.1.0",
  "description": "Decodes HCI packets sent over UART",
  "extensions": {
    "UART HCI": {
      "type": "HighLevelAnalyzer",
      "entryPoint": "hci.SerialHciHla"
    },
    "SPI HCI (BlueNRG/Zephyr)": {
      "type": "HighLevelAnalyzer",
      "entryPoint": "hci.BlueNrgHla"
    }
  }
}
